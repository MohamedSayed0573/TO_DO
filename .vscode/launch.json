{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug - Help",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/todo",
            "args": ["--help"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build Todo App",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "Debug - Show Tasks",
